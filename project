from enum import Enum

class Domain(Enum):
    EUKARYA = 1
    BACTERIA = 2
    ARCHAEA = 3

class Kingdom(Enum):
    ANIMALIA = 1
    FUNGI = 2
    EUBACTERIA = 3
    ARCHAEA = 4

    @property
    def domain(self):
        if self in (Kingdom.ANIMALIA, Kingdom.FUNGI):
            return Domain.EUKARYA
        elif self in (Kingdom.EUBACTERIA, Kingdom.ARCHAEA):
            return Domain.BACTERIA

class Phylum(Enum):
    CHORDATA = 1
    ARTHROPODA = 2
    MOLLUSCA = 3
    CNIDARIA = 4
    ECHINODERMATA = 5
    PLATYHELMINTHES = 6
    NEMATODA = 7
    ANNELIDA = 8
    ARTHROPODA = 9
    BACTERIA = 10
    ASCOMYCOTA = 11
    BASIDIOMYCOTA = 12
    CHYTRIDIOMYCOTA = 13
    GLOMEROMYCOTA = 14
    MUCOROMYCOTA = 15
    ZYGOMYCOTA = 16
    CRENARCHAEOTA = 17
    EURYARCHAEOTA = 18
    THAUMARCHAEOTA = 19
    KORARCHAEOTA = 20
    NANOARCHAEOTA = 21
    ACIDOBACTERIA = 22
    CHLAMYDIAE = 23
    CHLOROFLEXI = 24
    CYANOBACTERIA = 25
    FIRMICUTES = 26
    FUSOBACTERIA = 27
    PROTEOBACTERIA = 28
    SPIROCHAETES = 29
    SYNERGISTETES = 30
    TM7 = 31
    VERRUCOMICROBIA = 32

    @property
    def kingdom(self):
        if self in (Phylum.CHORDATA, Phylum.ARTHROPODA, Phylum.MOLLUSCA):
            return Kingdom.ANIMALIA
        elif self in (Phylum.CNIDARIA, Phylum.ECHINODERMATA, Phylum.PLATYHELMINTHES):
            return Kingdom.ANIMALIA
        elif self in (Phylum.NEMATODA, Phylum.ANNELIDA, Phylum.ARTHROPODA):
            return Kingdom.ANIMALIA
        elif self in (Phylum.BACTERIA, Phylum.ACIDOBACTERIA, Phylum.CHLAMYDIAE, Phylum.CHLOROFLEXI, Phylum.CYANOBACTERIA, Phylum.FIRMICUTES, Phylum.FUSOBACTERIA, Phylum.PROTEOBACTERIA, Phylum.SPIROCHAETES, Phylum.SYNERGISTETES, Phylum.TM7, Phylum.VERRUCOMICROBIA):
            return Kingdom.BACTERIA
        elif self in (Phylum.ASCOMYCOTA, Phylum.BASIDIOMYCOTA, Phylum.CHYTRIDIOMYCOTA, Phylum.GLOMEROMYCOTA, Phylum.MUCOROMYCOTA, Phylum.ZYGOMYCOTA):
            return Kingdom.FUNGI

class Class(Enum):
    MAMMALIA = 1
    INSECTA = 2
    GASTROPODA = 3
    ANTHOZOA = 4
    ASTEROIDEA = 5
    TURBELLARIA = 6
    DEMOSPONGIAE = 7
    CALCAREA = 8
    HEXACTINELLIDA = 9
    SCYPHOZOA = 10
    HYDROZOA = 11
    CUBOZOA = 12
    TREMATODA = 13
    CESTODA = 14
    SECERNENTEA = 15
    ADENOPHOREA = 16
    POLYCHAETA = 17
    CLITELLATA = 18
    INSECTA = 19
    ARACHNIDA = 20
    CRUSTACEA = 21
    MYRIAPODA = 22
    GASTROPODA = 23
    BIVALVIA = 24
    CEPHALOPODA = 25
    POLYPLACOPHORA = 26
    SCAPHOPODA = 27
    ANTHOZOA = 28
    ASTEROIDEA = 29
    OPHIUROIDEA = 30
    ECHINOIDEA = 31
    HOLOTHUROIDEA = 32
    CRINOIDEA = 33
    UROCHORDATA = 34
    ACIDOBACTERIA_CLASS1 = 35
    ACTINOBACTERIA_CLASS1 = 36
    BACTEROIDETES_CLASS1 = 37
    CHLAMYDIAE_CLASS1 = 38
    CHLOROBI_CLASS1 = 39
    CHLOROFLEXI_CLASS1 = 40
    CYANOBACTERIA_CLASS1 = 41
    FIRMICUTES_CLASS1 = 42
    FUSOBACTERIA_CLASS1 = 43
    PROTEOBACTERIA_CLASS1 = 44
    SPIROCHAETES_CLASS1 = 45
    SYNERGISTETES_CLASS1 = 46
    TM7_CLASS1 = 47
    VERRUCOMICROBIA_CLASS1 = 48

    @property
    def phylum(self):
        if self in (Class.MAMMALIA, Class.INSECTA):
            return Phylum.CHORDATA
        elif self in (Class.GASTROPODA, Class.BIVALVIA, Class.CEPHALOPODA, Class.POLYPLACOPHORA, Class.SCAPHOPODA):
            return Phylum.MOLLUSCA
        elif self in (Class.ANTHOZOA, Class.SCYPHOZOA, Class.HYDROZOA, Class.CUBOZOA):
            return Phylum.CNIDARIA
        elif self in (Class.ASTEROIDEA, Class.OPHIUROIDEA, Class.ECHINOIDEA, Class.HOLOTHUROIDEA, Class.CRINOIDEA):
            return Phylum.ECHINODERMATA
        elif self in (Class.TREMATODA, Class.CESTODA):
            return Phylum.PLATYHELMINTHES
        elif self in (Class.SECERNENTEA, Class.ADENOPHOREA):
            return Phylum.NEMATODA
        elif self in (Class.POLYCHAETA, Class.CLITELLATA):
            return Phylum.ANNELIDA
        elif self in (Class.ARACHNIDA, Class.CRUSTACEA, Class.MYRIAPODA):
            return Phylum.ARTHROPODA
        elif self in (Class.ACIDOBACTERIA_CLASS1, Class.ACTINOBACTERIA_CLASS1, Class.BACTEROIDETES_CLASS1, Class.CHLAMYDIAE_CLASS1, Class.CHLOROBI_CLASS1, Class.CHLOROFLEXI_CLASS1, Class.CYANOBACTERIA_CLASS1, Class.FIRMICUTES_CLASS1, Class.FUSOBACTERIA_CLASS1, Class.PROTEOBACTERIA_CLASS1, Class.SPIROCHAETES_CLASS1, Class.SYNERGISTETES_CLASS1, Class.TM7_CLASS1, Class.VERRUCOMICROBIA_CLASS1):
            return Phylum.BACTERIA
        elif self in (Class.ASCOMYCOTA, Class.BASIDIOMYCOTA, Class.CHYTRIDIOMYCOTA, Class.GLOMEROMYCOTA, Class.MUCOROMYCOTA, Class.ZYGOMYCOTA):
            return Phylum.FUNGI
